<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
    "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

    <bean id="product1" class="com.java.spr.Product">
        <property name="productName" value="Pen" />
        <property name="quantityAvail" value="10" />
        <property name="price" value="5.0" />
    </bean>

    <bean id="product2" class="com.java.spr.Product">
        <property name="productName" value="Notebook" />
        <property name="quantityAvail" value="5" />
        <property name="price" value="20.0" />
    </bean>

    <bean id="product3" class="com.java.spr.Product">
        <property name="productName" value="Pencil" />
        <property name="quantityAvail" value="15" />
        <property name="price" value="2.0" />
    </bean>

</beans>
